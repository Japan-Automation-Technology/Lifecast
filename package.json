{
  "name": "lifecast",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev:backend": "pnpm --filter @lifecast/backend dev",
    "dev:backend:worker:notifications": "pnpm --filter @lifecast/backend dev:worker:notifications",
    "dev:backend:worker:outbox": "pnpm --filter @lifecast/backend dev:worker:outbox",
    "build:backend": "pnpm --filter @lifecast/backend build",
    "typecheck:backend": "pnpm --filter @lifecast/backend typecheck",
    "test:backend": "pnpm --filter @lifecast/backend test",
    "smoke:payments:backend": "pnpm --filter @lifecast/backend smoke:payments",
    "db:migrate:backend": "pnpm --filter @lifecast/backend db:migrate",
    "db:seed:backend": "pnpm --filter @lifecast/backend db:seed",
    "db:bootstrap:backend": "pnpm --filter @lifecast/backend db:bootstrap"
  }
}
